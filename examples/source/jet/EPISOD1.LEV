IF C$ = CHR$(0) + "K" THEN x = x - 1: LOCATE y, x + 1: PRINT " ": a = 1
IF UCASE$(C$) = "Q" THEN SYSTEM
IF UCASE$(C$) = "X" THEN GOTO FINAL
IF SCREEN(y, x) = ASC("Û") THEN x = x - 1: LOCATE y, x + 1: PRINT "Û"
IF a$ = "si" AND SCREEN(y + 1, x) = ASC("ß") THEN TIMER OFF : a$ = "no"
IF SCREEN(y + 1, x) = ASC("ß") OR SCREEN(y + 1, x) = ASC("³") OR SCREEN(y + 1, x) = ASC("-") AND F = 0 THEN F = 0 ELSE F = 1
IF a = 0 AND SCREEN(y + 1, x - 1) = ASC("ß") AND SCREEN(y + 1, x) = ASC("#") THEN LOCATE y, x - 1: PRINT "I"
IF a = 0 AND SCREEN(y + 1, x - 1) = ASC("ß") AND SCREEN(y + 1, x - 2) = ASC("#") THEN LOCATE y, x - 1: PRINT "I"
